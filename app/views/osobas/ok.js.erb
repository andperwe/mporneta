$( "#komunikat_os" ).dialog({
              autoOpen: false,
              maxWidth:450,
              maxHeight: 220,
              width: 450,
              height: 220,
              modal: true,
              buttons:[
                      {
                        id:"ok_kom_os",
                        text: "Ok",
                        click: function() {
                          $('#komunikat_os').dialog("close");
                          $('#osobafd').dialog( "close" );
                          t.ajax.reload( null, false );
                          t5.ajax.reload(null, false);
                          $('#t_szukaj_o').val('');
                        }
                      }
                    ]
});

$("#ok_kom_os").mouseover(function() {
   $('button').removeClass("ui-state-hover");
});
$("#ok_kom_os").focus(function () {
    $(this).removeClass("ui-state-focus");
});
$('#ok_kom_os').removeClass();
$('#ok_kom_os').addClass('btn btn-success btn-sm');
t.ajax.reload();
t5.ajax.reload();
t6.ajax.reload();
$('#komunikat_os').html("<%= escape_javascript (render partial: 'ok') %>");
$('#komunikat_os').dialog( "open" );
