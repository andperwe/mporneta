if (nowa_agencja != 1){
$( "#komunikat_os" ).dialog({
              autoOpen: false,
              maxWidth:420,
              maxHeight: 200,
              width: 420,
              height: 200,
              modal: true,
              buttons:[
                      {
                        id:"ok_kom_ag",
                        text: "Ok",
                        click: function() {
                          $('#komunikat_os').dialog("close");
                          t8.ajax.reload( null, false );
                        }
                      }
                    ]
});

$("#ok_kom_ag").mouseover(function() {
   $('button').removeClass("ui-state-hover");
});
$("#ok_kom_ag").focus(function () {
    $(this).removeClass("ui-state-focus");
});
$('#ok_kom_ag').removeClass();
$('#ok_kom_ag').addClass('btn btn-success btn-sm');

$('#komunikat_os').html("<%= escape_javascript (render partial: 'u_ok') %>");
$('#komunikat_os').dialog( "open" );
}
