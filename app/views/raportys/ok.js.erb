$( "#komunikat_os" ).dialog({
              autoOpen: false,
              maxWidth:450,
              maxHeight: 220,
              width: 450,
              height: 220,
              modal: true,
              buttons:[
                      {
                        id:"ok_kom_np",
                        text: "Ok",
                        click: function() {
                          $('#komunikat_os').dialog("close");
                        }
                      }
                    ]
});

$("#ok_kom_np").mouseover(function() {
   $('button').removeClass("ui-state-hover");
});
$("#ok_kom_np").focus(function () {
    $(this).removeClass("ui-state-focus");
});
$('#ok_kom_np').removeClass();
$('#ok_kom_np').addClass('btn btn-success btn-sm');

if (raporty_email == 1){
  $('#komunikat_os').html("<%= escape_javascript (render partial: 'ok') %>");
}
else{
  $('#komunikat_os').html("<%= escape_javascript (render partial: 'okl') %>");
}
$('#komunikat_os').dialog( "open" );
